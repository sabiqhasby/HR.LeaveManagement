@inject NavigationManager NavigationManager

<button class="btn btn-outline-dark btn-block" @onclick="NavigateToList">
    <span class="bi bi-arrow-left-circle"></span>
    Back To List
</button>

@code {

    [Parameter]
    public required string Target { get; set; }

    void NavigateToList()
    {
        NavigationManager.NavigateTo($"{NavigationManager.BaseUri}{Target}");
    }

}